pipeline {
	agent any
	
	stages {
		stage('checkout') {
			steps {
				echo 'Hello World Samad Hyderabad'
			}
		}
	stage ('build')
    {
       steps 
    {
            echo 'hi there'	   
	}
		stage ('deploy')
     {
            steps
			
              echo 'hi hello'	   
	       }
	    }
    }
}
